<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">

    <servlet>
        <servlet-name>response</servlet-name>
        <servlet-class>com.liumqserv.servlet.ResponseDemo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>response</servlet-name>
        <url-pattern>/down</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>image</servlet-name>
        <servlet-class>com.liumqserv.servlet.ImageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>image</servlet-name>
        <url-pattern>/img</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>redi</servlet-name>
        <servlet-class>com.liumqserv.servlet.redirectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>redi</servlet-name>
        <url-pattern>/redi</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>requestTest</servlet-name>
        <servlet-class>com.liumqserv.servlet.RequestTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>requestTest</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>